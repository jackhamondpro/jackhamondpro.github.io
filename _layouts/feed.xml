---
layout: none
---

<?xml version="1.0" encoding="UTF-8" ?>
<rss version="2.0">
  <channel>
    <title>{{ site.title }}</title>
    <link>{{ site.url }}</link>
    <description>{{ site.description }}</description>
    <lastBuildDate>{{ site.time | date_to_xmlschema }}</lastBuildDate>
    <language>en-us</language>

    {% for project in site.projects %}
      <item>
        <title>{{ project.title }}</title>
        <link>{{ site.url }}{{ project.url }}</link>
        <description>{{ project.excerpt | strip_html | escape }}</description>
        <pubDate>{{ project.date | date_to_xmlschema }}</pubDate>
        <guid>{{ site.url }}{{ project.url }}</guid>
      </item>
    {% endfor %}
    
    <item>
      <title>About Me</title>
      <link>{{ site.url }}/about/</link>
      <description>{{ site.pages | where: "path", "about/index.md" | first.excerpt | strip_html | escape }}</description>
      <pubDate>{{ site.pages | where: "path", "about/index.md" | first.date | date_to_xmlschema }}</pubDate>
      <guid>{{ site.url }}/about/</guid>
    </item>
    
  </channel>
</rss>
